{
	"dynamoOptions": {
		"apiVersion": "2012-08-10",
		"region": "us-east-1",
		"endpoint": "https://dynamodb.us-east-1.amazonaws.com",
		"maxRetries": 3,
		"sslEnabled": true
	},
	"localDynamoOptions": {
		"region": "localhost",
		"endpoint": "http://localhost:8900"
	},
	"ProvisionedThroughput": {
		"ReadCapacityUnits": 25,
		"WriteCapacityUnits": 25
	},
	"serviceName": "dynamo-playground",
	"logLevel": "info"
}
